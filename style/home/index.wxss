page {
  background-color: #f4f4f4;
}
#azm-index .azm-promotion {
  height: 400rpx;
}
#azm-index .azm-promotion .weui-flex__item {
  height: 100%;
  box-sizing: border-box;
}
#azm-index .azm-promotion .weui-flex__item.right navigator {
  height: 50%;
  box-sizing: border-box;
  overflow: hidden;
}
#azm-index .azm-promotion .weui-flex__item image {
  width: 100%;
  height: 100%;
}
#azm-index .azm-row {
  background-color: #fff;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-bottom: 30rpx;
}
#azm-index .azm-row .h2 {
  padding-top: 32rpx;
  padding-bottom: 29rpx;
}
#azm-index .azm-row .h2 .zan-col {
  height: 40rpx;
  line-height: 40rpx;
}
#azm-index .azm-row .h2 .left {
  position: relative;
  padding-left: 20rpx;
}
#azm-index .azm-row .h2 .left text {
  font-size: 28rpx;
  color: #282828;
}
#azm-index .azm-row .h2 .left text.title {
  font-weight: 600;
}
#azm-index .azm-row .h2 .left:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 5rpx;
  height: 100%;
  border-radius: 2.5rpx;
  background-color: #ed397f;
}
#azm-index .azm-row .h2 .right {
  position: relative;
}
#azm-index .azm-row .h2 .right text {
  font-size: 24rpx;
  color: #888;
}
#azm-index .azm-row .h2 .right text.azm-icon {
  font-size: 20rpx;
  vertical-align: middle;
}
#azm-index .azm-row .azm-content {
  min-height: 100rpx;
  box-sizing: border-box;
}
#azm-index .azm-row .azm-content .azm-col {
  position: relative;
  box-sizing: border-box;
}
#azm-index .azm-row .azm-content .azm-col:nth-child(1) {
  padding-right: 12rpx;
}
#azm-index .azm-row .azm-content .azm-col image {
  width: 100%;
  height: 218rpx;
}
#azm-index .azm-row .azm-content .azm-col:nth-child(2) {
  padding-left: 6rpx;
  padding-right: 6rpx;
}
#azm-index .azm-row .azm-content .azm-col:nth-child(3) {
  padding-left: 12rpx;
}
#azm-index .azm-row .azm-content .azm-col .title {
  font-size: 28rpx;
  color: #282828;
  font-weight: 500;
  margin-top: 16rpx;
}
#azm-index .azm-row .azm-content .azm-col .money {
  font-size: 30rpx;
  color: #ff538b;
  margin-top: 23rpx;
}
#azm-index .azm-row .azm-content .azm-col .money .market_price {
  font-size: 24rpx;
  color: #999;
}
#azm-index view.azm-nearby_project {
  background-color: #fff;
}
#azm-index view.azm-nearby_project view {
  line-height: 1;
}
#azm-index view.azm-nearby_project .title-area {
  position: relative;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #fb5599;
  font-family: 'PingFang-SC-Bold';
}
#azm-index view.azm-nearby_project .title-area .azm-text {
  position: relative;
  display: inline-block;
}
#azm-index view.azm-nearby_project .title-area .azm-text .title_1_2 {
  width: 12rpx;
  height: 12rpx;
}
#azm-index view.azm-nearby_project .title-area .azm-text .title_1_2.left {
  left: -34rpx;
}
#azm-index view.azm-nearby_project .title-area .azm-text .title_1_2.right {
  right: -34rpx;
}
#azm-index view.azm-nearby_project .title-area .azm-text .title_1_1 {
  width: 6rpx;
  height: 6rpx;
}
#azm-index view.azm-nearby_project .title-area .azm-text .title_1_1.left {
  left: -52rpx;
}
#azm-index view.azm-nearby_project .title-area .azm-text .title_1_1.right {
  right: -52rpx;
}
#azm-index view.azm-nearby_project .title_area_round {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  border-radius: 100%;
  background-image: linear-gradient(90deg, #ff5ea2 0%, #ed3980 70%, #db135e 83%, #e82b8d 100%), linear-gradient(#c939fe, #c939fe);
  background-blend-mode: normal, normal;
}
#azm-index .azm-search {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  background-color: rgba(255, 255, 255, 0.7);
  border-radius: 29rpx;
  float: left;
  font-size: 28rpx;
  color: #444;
  padding-left: 20rpx;
}

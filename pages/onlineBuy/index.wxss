page {
    background-color: #f1f1f1;
}

.box {
    color: #222;
    border-radius: 10rpx;
    box-shadow: 0px 3px 14px 1px rgba(0, 0, 0, 0.05);
    background-color: #f9f9f9;
    width: 690rpx;
    margin: 0 auto;
}

.wbox {
    /* width: 690rpx; */
    margin: 0 auto;
    width: 100%;
    padding-left: 30rpx;
    padding-right: 30rpx;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
}

.t-remark {
    font-size: 24rpx;
}

.color-gray {
    /* color: #444; */
}

.pt {
    padding: 35rpx 30rpx;
    box-sizing: border-box;
}

.mt {
    margin: 35rpx -28rpx;
    height: 1rpx;
}

.tcolor {
    color: #666666;
    font-size: 28rpx;
}

.discount {}

.discount image {
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
    position: relative;
    top: -1rpx;
}

.fz30 {
    font-size: 30rpx;
}

.text-gray {
    color: #999;
}

#discount_area {
    margin-bottom: 30rpx;
}

.box2 {
    box-shadow: none;
    position: relative;
    overflow: hidden;
    padding-left: 40rpx;
    padding-right: 40rpx;
    background: transparent;
    width: 708rpx;
    box-sizing: border-box;
}

.box2-inner {
    z-index: 2;
    position: relative;
    margin-bottom: 60rpx;
}

.inputControl {
    border: none;
    font-size: 72rpx;
    width: 100%;
    color: #333333;
    height: 62rpx;
    text-align: right;
}

.main-btn {
    background-color: #16d6c3;
    height: 100rpx;
    line-height: 100rpx;
    width: 690rpx;
    text-align: center;
    font-size: 36rpx;
    white-space: nowrap;
    color: #fff;
    overflow: hidden;
    text-overflow: ellipsis;
    margin: 0 auto;
    margin-top: 57rpx;
    border-radius: 10rpx;
}

.main-btn.disabled {
    opacity: 0.4;
}

#actMoney b {
    font-size: 0.5866666666666667rem;
}

.addr {
    width: 16rpx;
    height: 25rpx;
    margin-right: 10rpx;
    vertical-align: middle;
}

.gt {
    width: 11rpx;
    height: 21rpx;
    margin-left: 24rpx;
}

.mask {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    z-index: 1;
}

#actMoney b {
    font-size: 44rpx;
}

.shopPay {
    height: 90rpx;
    font-size: 28rpx;
    color: #999999;
    display: flex;
    align-items: center;
    justify-content: center;
}

.shopPay>image {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    margin-right: 10rpx;
    /* margin-left: 190rpx; */
}

.t-expend {
    padding:  30rpx;
    background-color: #ffffff;
    box-sizing: border-box;
    font-size: 30rpx;
    color: #333333;
}

.t-noJoin {
    padding: 30rpx;
    box-sizing: border-box;
    background-color: #ffffff;
    font-size: 30rpx;
    color: #333333;
}

.t-discount {
    background-color: #ffffff;
}

.t-howDiscount {
    display: flex;
    align-items: center;
    height: 80rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;
    border-bottom: 1px solid #e5e5e5;
    color: #333333;
    font-size: 30rpx;
}

.t-payment {
    padding-right: 30rpx;
    background-color: #ffffff;
    display: flex;
    justify-content: flex-end;
    height: 80rpx;
    line-height: 80rpx;
}

.t-payment>text:first-child {
    font-size: 32rpx;
    color: #666666;
}

.t-payment>text:last-child {
    font-size: 36rpx;
    color: #16d6c3;
}

/* ---------- 自定义键盘 ----------- */

.flex {
    display: flex;
    align-items: center;
}

.t-keyBoard {
    position: fixed;
    bottom: 0;
    width: 100%;
    background-color: #ffffff;
}

.t-keyBoardNum text {
    display: inline-block;
    font-size: 60rpx;
    text-align: center;
    color: #222222;
    height: 130rpx;
    line-height: 130rpx;
    border-bottom: 1px solid #e5e5e5;
    border-right: 1px solid #e5e5e5;
    width: 33.3333333%;
    box-sizing: border-box;
}

.t-keyBoardNum {
    flex: 3;
    flex-wrap: wrap;
}

.t-keyBoardPay {
    flex: 1;
}

.t-keyBoardPay {
    flex-wrap: wrap;
    width: 100%;
    height: 100%;
}

.t-keyBoardPay text {
    width: 100%;
}

.t-keyBoardPay .t-reboke {
    height: 130rpx;
    width: 100%;
    text-align: center;
    line-height: 130rpx;
}

.t-keyBoardPay .t-reboke image {
    width: 52rpx;
    height: 33rpx;
}

.t-keyBoardPay .t-fixPayment {
    height: 390rpx;
    background-color: #16d6c3;
    width: 100%;
    overflow: hidden;
    text-align: center;
}

.t-fixPayment>text {
    font-size: 44rpx;
    color: #ffffff;
    display: block;
}

.t-fixPayment>text:first-child {
    margin-top: 154rpx;
}
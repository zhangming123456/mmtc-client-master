@import "../../style/activity/wallet.wxss";

.bg2 {
  width: 100%;
  height: 976rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  z-index: -1;
}

page {
  background-color: #feece1;
  text-align: center;
}

.line {
  width: 653rpx;
  height: 32rpx;
}

.box1 {
  padding-top: 660rpx;
}

.box1-inner {
  width: 655rpx;
  margin-top: 34rpx;
  background-color: rgba(255, 255, 255, 0.6);
  border-radius: 10rpx;
  display: inline-block;
  padding: 43rpx;
  font-size: 26rpx;
  color: #333;
  text-align: left;
  box-sizing: border-box;
  margin-bottom:20rpx;
}

.box1-item {
  margin-bottom: 31rpx;
}

.box1-item:last-child {
  margin-bottom: 0;
}

.box1-username {
  color: #ff9314;
}

.cash-btn {
  width: 655rpx;
  height: 99rpx;
  line-height: 99rpx;
  background-color: #ffa414;
  text-align: center;
  color: white;
  font-size: 40rpx;
  letter-spacing: 2rpx;
  border-radius: 10rpx;
  margin-bottom: 79rpx;
}

.masker {
  background-color: rgba(0, 0, 0, 0.8);
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}

.close-btn {
  width: 72rpx;
  height: 72rpx;
}

.masker-wrap {
  margin-top: 168rpx;
  padding: 0 25rpx;
}

.masker-inner {
  background-color: #feece0;
  box-shadow: 0px 6rpx 20rpx 0 rgba(0, 0, 0, 0.3);
  border-radius: 10rpx;
  padding: 69rpx 0 89rpx 0;
  text-align: center;
}

image.tipimg {
  width: 683rpx;
  height: 642rpx;
}

.mask-title {
  text-align: left;
  font-size: 48rpx;
  color: #feece0;
}

.masker-header {
  padding-bottom: 30rpx;
}

.top-yuan {
  color: #d94d43;
  font-size: 32rpx;
  line-height: 1;
}

.top-yuan-money {
  font-size: 107rpx;
  display: inline;
}

.top-info{
  position: absolute;
  top:208rpx;
  text-align: center;
  left:0;
  right:0;
}


view {
    line-height: normal;
}

page {
    background-color: #F0F5F4;
}

.t-cardCode {
    padding: 30rpx 30rpx 70rpx;
    background-color: #ffffff;
    box-shadow: 0 5rpx 9rpx 0 rgba(0, 0, 0, 0.07);
    border-radius: 10rpx;
    margin: 30rpx;
}

.t-shopName {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    color: #333333;
}

.t-code {
    text-align: center;
    margin-top: 70rpx;
}

.t-code>image {
    width: 257rpx;
    height: 255rpx;
}

.t-xfCode {
    font-size: 28rpx;
    color: #323232;
    margin-top: 50rpx;
    text-align: center;
}

.t-xfCode2 {
    font-size: 28rpx;
    color: #C7C7C7;
    margin-top: 50rpx;
    text-align: center;
}

.t-xfCode>text:last-child {
    color: #04D3AF;
}

.t-order {
    font-size: 24rpx;
    color: #666666;
    text-align: center;
    margin-top: 10rpx;
}

.t-order2 {
    font-size: 24rpx;
    color: #C7C7C7;
    text-align: center;
    margin-top: 10rpx;
}
page {
  /* background-color: #f4f4f4; */
}
.mineMesssage {
  box-sizing: border-box;
  width: 100%;
  height: 278rpx;
  padding-left: 37rpx;
  padding-right: 37rpx;
  padding-top: 30rpx;
  position: relative;
  background-color: #f4f4f4;
}
.mineBg {
  width: 100%;
  height: 100%;
}
.h20 {
  height: 20rpx;
  width: 100%;
  border-bottom: 1px solid #EEEEEE;
  border-top: 1px solid #EEEEEE;
  background-color: #f4f4f4;
}
.mineUser {
  position: absolute;
  top: 60%;
  z-index: 1;
  transform: translate(0%, -60%);
  left: 85rpx;
  display: flex;
  align-items: center;
}
.mineUser image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 33rpx;
}
.mineLogin {
  font-size: 32rpx;
  color: #FFFFFF;
}
.quitLogin {
  border: 2rpx solid #FFFFFF;
  border-radius: 30rpx;
  width: 120rpx;
  height: 60rpx;
  font-size: 28rpx;
  color: #FFFFFF;
  line-height: 60rpx;
  text-align: center;
  position: absolute;
  right: 80rpx;
  top: 60%;
  transform: translate(0%, -60%);
}
.mineMenu {
  background: #FFFFFF;
  padding-top: 30rpx;
  padding-bottom: 37rpx;
}
.mineModule {
  width: 100%;
  display: flex;
}
.mineModule:last-child {
  margin-top: 22rpx;
}
.clickModule {
  text-align: center;
  width: 25%;
}
.clickModule image {
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 20rpx;
}
.clickModule view {
  font-size: 24rpx;
  color: #999999;
}
.minePerson {
  padding-top: 32rpx;
  padding-bottom: 24rpx;
  box-sizing: border-box;
  width: 100%;
  background-color: #FFFFFF;
  display: flex;
}
.mineTrack {
  width: 25%;
  text-align: center;
}
.mineTrack image {
  width: 40rpx;
  margin-bottom: 20rpx;
  height: 40rpx;
}
.mineTrack view {
  font-size: 24rpx;
  color: #999999;
}
.withMine {
  width: 100%;
  display: flex;
  align-items: center;
  height: 90rpx;
  justify-content: space-between;
  border-bottom: 1px solid #EEEEEE;
  font-size: 28rpx;
  color: #000000;
  font-weight: 500;
  padding-left: 37rpx;
  padding-right: 37rpx;
  box-sizing: border-box;
}
.withMine image {
  width: 25rpx;
  height: 25rpx;
}
.support {
  position: absolute;
  bottom: 43rpx;
  font-size: 26rpx;
  color: #999999;
  text-align: center;
  left: 50%;
  transform: translate(-50%, 0%);
  width: 100%;
}
.azm-userInfo-login-btn {
  display: none;
}
.azm-mine .azm-userInfo-avatar {
  width: 170rpx;
  height: 170rpx;
  border-radius: 100%;
  overflow: hidden;
}
.azm-mine .azm-userInfo {
  box-sizing: border-box;
  padding-left: 30rpx;
}
.azm-mine .azm-userInfo .azm-userInfo-text {
  font-size: 25rpx;
  color: #000;
}
.azm-mine .azm-userInfo .azm-userInfo-nickName {
  padding-top: 12rpx;
  font-size: 28rpx;
  color: #999;
}
.azm-mine .azm-exit-btn {
  width: 130rpx;
  line-height: 43rpx;
  padding: 0;
  border-radius: 43rpx;
  font-size: 24rpx;
  border-color: #999;
  color: #999;
}
.azm-mine .azm-mine-info .azm-mine-info-title,
.azm-mine .azm-mine-info .azm-mine-info-box {
  position: relative;
  box-sizing: border-box;
  padding: 24rpx 30rpx;
}
.azm-mine .azm-mine-info .azm-mine-info-title {
  font-size: 32rpx;
  color: #282828;
  padding-left: 54rpx;
}
.azm-mine .azm-mine-info .azm-mine-info-title:before {
  content: ' ';
  position: absolute;
  top: 50%;
  left: 30rpx;
  transform: translate(0, -50%);
  width: 6rpx;
  height: 24rpx;
  background-color: #fd0f5d;
}
.azm-mine .azm-mine-info .azm-mine-info-box {
  text-align: center;
}
.azm-mine .azm-mine-info .azm-mine-info-box text {
  font-size: 28rpx;
  color: #666;
}
.azm-mine .azm-mine-info .azm-mine-info-box .azm-icon {
  padding-bottom: 10rpx;
}
.azm-mine .azm-mine-info .azm-mine-info-box .azm-icon text {
  font-size: 38rpx;
  color: #999;
}
.azm-mine .azm-cell-icon {
  width: 44rpx;
  height: 38rpx;
  margin-right: 20rpx;
  vertical-align: text-bottom;
}
.azm-mine i-cell view {
  font-size: 28rpx;
  color: #000000;
  font-weight: 500;
}
.azm-mine .azm-footer {
  font-size: 24rpx;
  text-align: center;
  font-weight: normal;
  font-stretch: normal;
  line-height: 36rpx;
  color: #999;
  padding-top: 33rpx;
  padding-bottom: 20rpx;
}

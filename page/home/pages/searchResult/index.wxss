page {
  width: 100%;
  height: 100%;
  background-color: #f4f4f4;
}

.searchBox {
  padding: 20rpx 37rpx;
  box-sizing: border-box;
  background-color: #ffffff;
  box-shadow: 0 5rpx 10rpx 0 rgba(219, 219, 219, 0.50);
}

.search {
  background-color: rgba(0, 0, 0, 0.08);
  border-radius: 29rpx;
  height: 58rpx;
  line-height: 58rpx;
  width: 100%;
  font-size: 24rpx;
  color: #999;
  box-sizing: border-box;
  padding-left: 30rpx;
  display: flex;
  align-items: center;
}

.search-img {}

.search-input {
  width: 100%;
  padding-left: 20rpx;
  height: 23rpx;
  line-height: 23rpx;
  margin-top: 5rpx;
  display: inline-block;
  box-sizing: border-box;
}

.cancel-text {
  font-size: 29rpx;
  color: #04D3AF;
  margin-left: 29rpx;
}

.searchResult {
  display: flex;
  padding: 20rpx 37rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #dddddd;
  position: relative;
}



.Location {
  position: absolute;
  right: 37rpx;
  width: 37rpx;
  height: 37rpx;
}

.searchResult .shopPhone {
  width: 155rpx;
  height: 155rpx;
  border-radius: 10rpx;
}


.searchName {
  margin-left: 18rpx;
  flex: 1;
}

.shopName {
  display: flex;
  align-items: center;
  font-size: 29rpx;
  color: #333333;
  margin-bottom: 10rpx;
}




.shopSite {
  font-size: 26rpx;
  color: #999999;
  margin-bottom: 52rpx;
}

.shopLocation {
  font-size: 26rpx;
  color: #666666;
  text-align: right;
  float: right;
}